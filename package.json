{
  "name": "swapi-apollo-server",
  "version": "0.0.0",
  "description": "swapi-graphql served over apollo-server",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "npm install",
    "build": "webpack",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:notify": "npm run test:watch -- --notify",
    "coverage": "npm test -- --coverage",
    "coverage:notify": "npm run coverage -- --watch --notify",
    "start": "npm run build && node dist/main.js",
    "preupver": "npm test",
    "upver": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DxCx/swapi-apollo-server.git"
  },
  "keywords": [
    "apollo",
    "backend",
    "apollo-server",
    "typescript",
    "webpack",
    "graphql",
    "graphiql",
    "express"
  ],
  "author": "Hagai Cohen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DxCx/swapi-apollo-server/issues"
  },
  "homepage": "https://github.com/DxCx/swapi-apollo-server#readme",
  "dependencies": {
    "@types/express": "^4.0.31",
    "@types/hapi": "^13.0.33",
    "@types/helmet": "0.0.30",
    "@types/koa": "^2.0.31",
    "@types/morgan": "^1.7.30",
    "body-parser": "^1.15.2",
    "dataloader": "^1.2.0",
    "express": "^4.14.0",
    "graphql": "^0.9.1",
    "graphql-rxjs": "^0.9.1-rc.1",
    "graphql-server-core": "../../GraphQL/graphql-server/packages/graphql-server-core",
    "graphql-server-express": "../../GraphQL/graphql-server/packages/graphql-server-express",
    "graphql-server-module-graphiql": "../../GraphQL/graphql-server/packages/graphql-server-module-graphiql",
    "graphql-server-observable": "../../GraphQL/graphql-server/packages/graphql-server-observable",
    "graphql-server-reactive-core": "../../GraphQL/graphql-server/packages/graphql-server-reactive-core",
    "graphql-server-reactive-protocol": "../../GraphQL/graphql-server/packages/graphql-server-reactive-protocol",
    "graphql-server-ws": "../../GraphQL/graphql-server/packages/graphql-server-ws",
    "helmet": "^2.1.2",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.7.0",
    "rxjs": "^5.2.0",
    "swapi-graphql": "0.0.6",
    "ws": "^1.1.4"
  },
  "devDependencies": {
    "@types/graphql": "^0.8.6",
    "@types/isomorphic-fetch": "0.0.31",
    "@types/jest": "^0.9.30",
    "@types/node": "^6.0.38",
    "@types/ws": "0.0.33",
    "awesome-typescript-loader": "^3.1.2",
    "jest": "^15.1.1",
    "standard-version": "^2.4.0",
    "ts-jest": "^0.1.5",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.3.3"
  },
  "jest": {
    "scriptPreprocessor": "node_modules/ts-jest/preprocessor.js",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
